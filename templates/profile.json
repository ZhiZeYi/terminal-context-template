{
  "version": "1.0.0",
  "lastUpdated": "2026-01-05",
  "author": "TCM Community",
  "description": "Terminal Context Manager - Profile Templates & Environment Variables Dictionary",
  "repository": "https://github.com/user/tcm-templates",
  "templates": [
    {
      "id": "git-dev",
      "name": "Git Development",
      "description": "Git version control configuration for development workflow",
      "category": "Development",
      "icon": "git-branch",
      "tags": [
        "git",
        "vcs",
        "development"
      ],
      "variables": [
        {
          "key": "GIT_AUTHOR_NAME",
          "type": "literal",
          "description": "Your name for git commits"
        },
        {
          "key": "GIT_AUTHOR_EMAIL",
          "type": "literal",
          "description": "Your email for git commits"
        },
        {
          "key": "GIT_COMMITTER_NAME",
          "type": "literal",
          "description": "Committer name (usually same as author)"
        },
        {
          "key": "GIT_COMMITTER_EMAIL",
          "type": "literal",
          "description": "Committer email"
        },
        {
          "key": "GIT_EDITOR",
          "value": "code --wait",
          "type": "literal",
          "description": "Editor for git commit messages"
        },
        {
          "key": "GIT_SSH_COMMAND",
          "value": "ssh -o StrictHostKeyChecking=no",
          "type": "literal",
          "description": "SSH command for git"
        }
      ]
    },
    {
      "id": "ai-dev",
      "name": "AI Development",
      "description": "API keys and configuration for AI/ML services",
      "category": "AI",
      "icon": "hubot",
      "tags": [
        "ai",
        "openai",
        "anthropic",
        "llm"
      ],
      "variables": [
        {
          "key": "OPENAI_API_KEY",
          "type": "secretRef",
          "description": "OpenAI API key"
        },
        {
          "key": "OPENAI_ORG_ID",
          "type": "literal",
          "description": "OpenAI organization ID"
        },
        {
          "key": "ANTHROPIC_API_KEY",
          "type": "secretRef",
          "description": "Anthropic Claude API key"
        },
        {
          "key": "AZURE_OPENAI_ENDPOINT",
          "type": "literal",
          "description": "Azure OpenAI endpoint URL"
        },
        {
          "key": "AZURE_OPENAI_API_KEY",
          "type": "secretRef",
          "description": "Azure OpenAI API key"
        },
        {
          "key": "GEMINI_API_KEY",
          "type": "secretRef",
          "description": "Google Gemini API key"
        },
        {
          "key": "HUGGINGFACE_TOKEN",
          "type": "secretRef",
          "description": "HuggingFace API token"
        }
      ]
    },
    {
      "id": "claude-cli-deepseek",
      "name": "Claudecode/deepseek / Anthropic (Claude) CLI",
      "description": "Environment variables for using claudecode-style CLI with Anthropic or Deepseek Anthropic proxy",
      "category": "AI",
      "icon": "terminal",
      "tags": [
        "claudecode",
        "anthropic",
        "deepseek",
        "cli"
      ],
      "variables": [
        {
          "key": "ANTHROPIC_AUTH_TOKEN",
          "type": "secretRef",
          "description": "Anthropic API key (used by claudecode / CLI). Do NOT commit the real key."
        },
        {
          "key": "ANTHROPIC_BASE_URL",
          "value": "https://api.deepseek.com/anthropic",
          "type": "literal",
          "description": "Base URL for Anthropic or proxy endpoint (e.g., Deepseek). Adjust to provider. https://api.deepseek.com/anthropic "
        },
        {
          "key": "ANTHROPIC_MODEL",
          "value": "deepseek-chat",
          "type": "literal",
          "description": "Default model to use with the CLI (e.g., deepseek-chat, claude-2)"
        },
        {
          "key": "ANTHROPIC_SMALL_FAST_MODEL",
          "value": "deepseek-small",
          "type": "literal",
          "description": "Optional smaller/faster model name"
        },
        {
          "key": "CLAUDECODE_CONFIG_PATH",
          "value": "~/.config/claudecode/config.yaml",
          "type": "literal",
          "description": "Path to local claudecode CLI config (optional)"
        },
        {
          "key": "CLAUDECODE_PROFILE",
          "type": "literal",
          "description": "Optional claudecode profile name to select (if CLI supports profiles)"
        },
        {
          "key": "CLAUDECODE_DEFAULT_TIMEOUT_MS",
          "value": "30000",
          "type": "literal",
          "description": "Default request timeout for claudecode CLI (milliseconds)"
        }
      ]
    },
    {
      "id": "aws-cloud",
      "name": "AWS Cloud",
      "description": "Amazon Web Services credentials and configuration",
      "category": "Cloud",
      "icon": "cloud",
      "tags": [
        "aws",
        "cloud",
        "amazon"
      ],
      "variables": [
